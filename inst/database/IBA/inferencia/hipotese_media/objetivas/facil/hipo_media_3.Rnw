## QUESTION/ANSWER GENERATION
questions <- solutions <- NULL

questions[1] <- '$\\\\mu$.'
solutions[1] <- TRUE

questions[2] <- '$\\\\pi$.'
solutions[2] <- FALSE

questions[3] <- '$\\\\sigma$.'
solutions[3] <- FALSE

questions[4] <- '$\\\\sigma^2$.'
